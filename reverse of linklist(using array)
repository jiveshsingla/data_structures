#include<stdio.h>
#include<stdlib.h>
struct Node
{
        int data;
        struct Node* next;
};
void push(int n,struct Node** head)
{
    int i;
        for(i=n;i>0;i--)
        {
                struct Node* temp=NULL;
                temp=(struct Node*)malloc(sizeof(struct Node));
                temp->data=i;
                temp->next=*head;
                *head=temp;
        }
}
void reverse(int arr[],struct Node* temp1,int n)
{
    int i=0;
    while(temp1!=NULL)
        {
                printf("%d ",temp1->data);
                arr[i]=temp1->data;
                temp1=temp1->next;
                i++;
        }
        for(i=n-1;i>=0;i--)
        {
                printf("%d ",arr[i]);
        }
}
int main(int argc,char* argv[])
{
        struct Node* head=NULL;
        int n;
        scanf("%d",&n);
        push(n,&head);
        struct Node* temp1=head;
        int arr[n];
        reverse(arr,temp1,n);
        return 0;
}
