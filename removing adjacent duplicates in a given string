#include<iostream>
using namespace std;
int main()
{
    int n=0;
    cout<<"enter testcases"<<endl;
    cin>>n;
    int i,j,flag=0;
    for(i=0;i<n;i++)
    {
        string str="";
        cout<<"enter string"<<endl;
        cin>>str;
        int l=str.length();
        for(j=1;j<l;j++)
        {
            if(str[j]==str[j-1])
            {
                str[j-1]=' ';
                flag=1;
            }
            else
            {
                if(flag==1)
                {
                str[j-1]=' ';
                flag=0;
                }
            }
        }
        if(flag==1)
        str[l-1]=' ';
        for(j=0;j<l;j++)
        {
            if(str[j]!=' ')
            cout<<str[j];
        }
        cout<<endl;
    }
    return 0;
}
