#include<iostream>
#include<cstring>
int size=256;
using namespace std;
int main(int argc,char* argv[])
{
    int count=0;
    string str1="listen";
    string str2="silent";
    char temp[size]={0};
    int len1=str1.length();
    int len2=str2.length();
    if(len1!=len2)
    cout<<"NO";
    else
    {
    for(int i=0;i<len1;i++)
    {
        temp[str1[i]]+=1;;
    }
    for(int i=0;i<len2;i++)
    {
        temp[str2[i]]+=1;;
    }
    for(int i=0;i<size;i++)
    {
        if(temp[i]==2)
        count++;
    }
    if(count==len1)
    cout<<"YES";
    else
    cout<<"NO";
    }
    return 0;
}
