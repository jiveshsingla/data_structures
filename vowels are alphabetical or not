#include<stdio.h>
#include<string.h>
int main(int argc,char* argv[])
{
 char* str="aabbddiiieecc";
 int n=strlen(str);
 char vowels[100];
 int flag=0,k=0;
 
 for(int i=0;i<n;i++)
 {
     if(str[i]=='a'|| str[i]=='e'|| str[i]=='i'||str[i]=='o'||str[i]=='u')
     {
         vowels[k]=str[i];
         k++;
     }
 }
    for(int i=1;i<k;i++)
     {
         if(vowels[i]>=vowels[i-1])
         {
             flag=0;
         }
         else
         {
             flag=1;
             break;
         }
     }
 
if(flag==1)
{
    printf("unsorted");
}
else
{
    printf("sorted");
}

 return 0;
}
