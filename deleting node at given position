#include<iostream>
using namespace std;
class Node
{
    public:
    int data;
    Node* next;
};
Node* delete_node(Node* head,int pos)
{
    Node* temp=head;
    int count=0;
    if(pos==0)
    {
        head=head->next;
    }
        else
        {
            while(temp!=NULL)
            {
                if(count==pos-1)
                break;
                count++;
                temp=temp->next;
            }
            temp->next=temp->next->next;
        }
        return head;
}
void print_node(Node* head)
{
    while(head->next!=0)
    {
        cout<<head->data<<" ";
        head=head->next;
    }
    cout<<head->data;
}
int main(int argc,char* argv[])
{
    int pos=atoi(argv[1]);
    Node* head=NULL;
    for(int i=argc-1;i>1;i--)
    {
        Node* temp=new Node();
        temp->data=atoi(argv[i]);
        temp->next=head;
        head=temp;
    }
    head=delete_node(head,pos);
    print_node(head);
    return 0;
}
