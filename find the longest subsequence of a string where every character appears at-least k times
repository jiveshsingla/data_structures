#include<iostream>
#include<stdlib.h>
#include<string>
int size=256;
using namespace std;
int main(int argc,char* argv[])
{
    int num=atoi(argv[1]);
    string str=argv[2];
    int len=str.length();
    int i;
    char temp[size]={0};
    for(i=0;i<len;i++)
    {
        temp[str[i]]+=1;
    }
    string str1="";
    for(i=0;i<len;i++)
    {
        if(temp[str[i]]>=num)
        str1+=str[i];
    }
    cout<<str1;
    return 0;
}
