#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int main(int argc,char* argv[])
{
    char* str="Hello World";
    int n=strlen(str);
    for(int i=0;i<n;i++)
    {
        if(str[i]>='a' && str[i]<='z')
        printf("%c",str[i]);
    }
    for(int i=0;i<n;i++)
    {
        if(str[i]>='A' && str[i]<='Z')
        printf("%c",str[i]);
    }
    return 0;
}

