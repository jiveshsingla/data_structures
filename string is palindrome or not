#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int main(int argc,char* argv[])
{
    char* str="aabbaa";
    int n=strlen(str);
    int k=n-1,flag=0;
    int i=0;
    while(i<k)
    {
        if(str[i]!=str[k])
        {
        flag=1;
        break;
        }
        k--;
        i++;
    }
    if(flag==1)
    printf("NO");
    else
    printf("YES");
    return 0;
}
