#include<iostream>
#include<cstring>
using namespace std;
void swap(char *x,char *y)
{
    char temp=*x;
    *x=*y;
    *y=temp;
}
int main(int argc,char* argv[])
{
    string str1(argv[1]);
    string str2(argv[2]);
    int i,j,flag=0;
    int len1=str1.length();
    int len2=str2.length();
    if(len1!=len2)
    cout<<"NO";
    else
    {
    for(i=0;i<len1;i++)
    {
        for(j=i+1;j<len1;j++)
        if(str1[i]>str1[j])
        swap(&str1[i],&str1[j]);
    }
    for(i=0;i<len2;i++)
    {
        for(j=i+1;j<len2;j++)
        if(str2[i]>str2[j])
        swap(&str2[i],&str2[j]);
    }
    for(i=0;i<len1;i++)
    {
        if(str1[i]!=str2[i]){
        flag=1;
        break;}
    }
    if(flag==1)
    cout<<"NO";
    else
    cout<<"YES";
    }
    return 0;
}
