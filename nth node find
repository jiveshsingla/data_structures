#include<iostream>
using namespace std;
class Node
{
    public:
    int data;
    Node* next;
};
void find_node(Node* temp,int pos)
{
    int count=1;
            while(temp!=NULL)
            {
                if(count==pos)
                break;
                count++;
                temp=temp->next;
            }
            cout<<temp->data;
}
int main(int argc,char* argv[])
{
    int pos=atoi(argv[1]);
    Node* head=NULL;
    for(int i=argc-1;i>1;i--)
    {
        Node* temp=new Node();
        temp->data=atoi(argv[i]);
        temp->next=head;
        head=temp;
    }
    find_node(head,pos);
    return 0;
}
