
#include<iostream>
using namespace std;
int main(int argc,char* argv[])
{
    string str=argv[1];
    string str1=str;
    int i=0,j=1;
    int len=str.length();
    while(str[j])
    {
        if(str[i]!=str[j])
        {
            ++i;
            char temp=str[i];
            str[i]=str[j];
            str[j]=temp;
        i++;
        j=i+1;
        }
        else 
        {
            if(str[i]==str[j])
            j++;
        }
    }
    if(j==i+1)
    {
        cout<<str;
    }
    else
    {
        cout<<str1;
    }
 return 0;   
}
