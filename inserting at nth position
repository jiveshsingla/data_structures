#include<iostream>
using namespace std;
class Node
{
    public:
    int data;
    Node* next;
};
Node* insert_element(Node* head,int num,int pos)
{
    int count=0;
    Node* temp=new Node();
    temp->data=num;
    Node* temp1=head;
    if(pos==0)
    {
        temp->next=head;
        head=temp;
    }
    else
    {
        while(temp1!=NULL)
        {
        if(count==pos-1)
        break;
        count++;
        temp1=temp1->next;
        }
        temp->next=temp1->next;
        temp1->next=temp;
    }
    return head;
}
void print(Node* head)
{
    while(head->next!=NULL)
    {
        cout<<head->data<<" ";
        head=head->next;
    }
    cout<<head->data;
}
int main(int argc,char* argv[])
{
    int num=atoi(argv[1]);
    int pos=atoi(argv[2]);
    Node* head=NULL;
    for(int i=argc-1;i>2;i--)
    {
        Node* temp=new Node();
        temp->data=atoi(argv[i]);
        temp->next=head;
        head=temp;
    }
    head=insert_element(head,num,pos);
    print(head);
    return 0;
}
