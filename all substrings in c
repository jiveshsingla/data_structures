#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int main(int argc,char* argv[])
{
    char* str="jivesh";
    int i,j,k=1,l=k,m;
    int n=strlen(str);
    for(i=0;i<n;i++)
    printf("%c ",str[i]);
    for(i=0;i<n-2;i++)
    {
        for(j=0;j<n-k;j++)
        {
            for(m=j;m<=l;m++)
            printf("%c",str[m]);
            printf(" ");
            l++;
        }
        k++;
        l=k;
    }
    printf(" %s",str);
    return 0;
}
